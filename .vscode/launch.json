{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach by Process ID",
            "processId": "${command:PickProcess}",
            "request": "attach",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node",
            "trace": true,
            "outFiles": ["${workspaceFolder}/dist/**/*.js"],
        },
    ]
}
